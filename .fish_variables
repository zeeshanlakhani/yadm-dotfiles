set -gx PATH $HOME/.emacs.d/bin $PATH
set -gx PATH $HOME/.nimble/bin $PATH
set -gx PATH $HOME/.elixirls/release $PATH

set -gx CARGO_HOME $HOME/.cargo

set -gx GOPATH $HOME/go
set -gx GOROOT /usr/local/go
