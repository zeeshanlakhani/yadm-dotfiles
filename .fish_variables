set --export PATH /usr/local/bin $PATH
set --export PATH /usr/local/opt/llvm/bin $PATH
set --export PATH /usr/local/sbin $PATH
set --export PATH $HOME/.cargo/bin $PATH
set --export PATH /usr/local/smlnj/bin $PATH
set --export PATH $HOME/.emacs.d/bin $PATH
set --export PATH $HOME/.nimble/bin $PATH
set --export PATH $HOME/.elixirls/release $PATH

set --export GOPATH $HOME/go
set --export GOROOT /usr/local/go
set --export PATH $GOPATH/bin $GOROOT/bin $PATH

set --export JAVA_HOME /usr/libexec/java_home -v 1.8
set --export PATH $JAVA_HOME/bin $PATH

set --export PATH /usr/local/opt/coreutils/libexec/gnubin $PATH
set --export PATH "/Applications/Racket v7.8/bin" $PATH
set --export PATH $PYENV_ROOT/bin $PATH

set --export PYENV_ROOT $HOME/.pyenv
set --export SDKROOT /Library/Developer/CommandLineTools/SDKs/MacOSX10.15.sdk
